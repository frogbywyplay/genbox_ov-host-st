+static void
+shtdi_load (char *prog, int fromtty)
+{
+  char *execFile = NULL, *execName = NULL;
+#ifndef __MINGW32__
+  static const char *sep = "/";
+#else
+  static const char *sep = ":/\\";
+#endif
+
+  shtdi_args_passed = 0;
+
+  execFile = get_exec_file (0);
+  if (!execFile)
+    execFile = prog;
+
+  if (execFile)
+    {
+      char *s = execFile;
+      while ((s = strpbrk (s, sep)) != NULL)
+	execName = ++s;
+
+      if (!execName)
+	execName = execFile;
+
+      xfree (shtdi_execName);
+      shtdi_execName = xstrdup (execName);
+    }
+
+  generic_load (prog, fromtty);
+}
+
+static void
+shtdi_create_inferior (char *exec_file, char *args, char **env, int from_tty)
+{
+  if (symfile_objfile != NULL)
+    rtos_check_symbols (symfile_objfile);
+
+  if (exec_file)
+    {
+      shtdi_load (exec_file, 0);
+      continue_command (NULL, 0);
+    }
+  else
+    {
+      int argslen = strlen (args);
+      char *savedargs = NULL;
+      char *execname = NULL;
+      char *execargs = NULL;
+
+      if (!*args)
+	error (_("No executable file to run"));
+
+      savedargs = xstrdup (args);	/* Save arguments for later */
+
+      execname = strtok (args, " ");
+      argslen -= strlen (execname) + 1;
+
+      if (argslen > 0)
+	xfree (set_inferior_args
+	       (xstrdup (execname + strlen (execname) + 1)));
+      else
+	xfree (set_inferior_args (xstrdup ("")));
+
+      shtdi_load (execname, 0);
+      continue_command (NULL, 0);
+
+      /* Reset args so run with no args works */
+      xfree (set_inferior_args (savedargs));
+    }
+}
