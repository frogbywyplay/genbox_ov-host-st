+/**************************************************************************/
+/* Signal Handling.  */
+
+/* The user has pressed Control-C,
+   but it has not yet been processed.  */
+static int interrupt_pending = 0;
+
+/* Remember the regular SIGINT handler while ours is installed.  */
+static void (*old_interrupt_handler) (int);
+
+/* Ask the user what to do when an interrupt is received.  */
+static void
+interrupt_query (void)
+{
+  target_terminal_ours ();
+
+  if (query (_("Interrupted while waiting for the program.\n"
+	       "Give up (and stop debugging it)? ")))
+    {
+      target_mourn_inferior ();
+      deprecated_throw_reason (RETURN_QUIT);
+    }
+
+  target_terminal_inferior ();
+}
+
+/* interrupt_once and interrupt_twice reference each other.  */
+static void interrupt_twice (int signo);
+
+/* The user typed ^C once.
+   Send the interrupt on to the target and hope it responds.  */
+static void
+interrupt_once (int signo)
+{
+  /* If this doesn't work, try more severe steps.  */
+  signal (signo, interrupt_twice);
+
+  if (remote_debug)
+    fprintf_unfiltered (gdb_stdlog, "interrupt_once called\n");
+
+  target_stop ();
+}
+
+/* The user typed ^C twice.  */
+static void
+interrupt_twice (int signo)
+{
+  signal (signo, old_interrupt_handler);
+  interrupt_query ();
+  signal (signo, interrupt_once);
+}
+
+/* This is called when the user wants to interrupt the target.
+   All we do is set interuptPending. The actual work is handled
+   by interrupt_check().  */
+static void
+shtdi_stop (void)
+{
+  if (interrupt_pending)
+    interrupt_query ();		/* If interrupt pending then ask
+				   if user wishes to quit */
+  else
+    interrupt_pending = 1;
+}
+
+/* Send an interrupt to the target.  */
+static void
+iointerrupt (void)
+{
+  char int_req = 0x03;
+  int result = 0;
+  int bytesWritten = 0;
+
+#if defined(__MINGW32__) || defined(__CYGWIN__)
+  result =
+    WriteFile ((HANDLE) ioconsole_writehandle, &int_req, 1,
+	       (DWORD *) & bytesWritten, NULL);
+#else
+  result = write (ioconsole_writehandle, &int_req, 1);
+#endif
+}
+
+/* Test if there is a queued interrupt waiting to be serviced.  */
+static void
+interrupt_check ()
+{
+  if (interrupt_pending)
+    {
+      interrupt_pending = 0;	/* Reset */
+      if (ioconsole_enabled)
+	iointerrupt ();
+
+      if (remote_debug)
+	fprintf_unfiltered (gdb_stdlog, "calling shdbg_Interrupt\n");
+
+      if (!shdbg_Interrupt ())
+	error (_("Unable to stop target"));
+    }
+}
