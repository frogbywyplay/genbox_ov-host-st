+static void
+shtdi_addrfromsymb (const char *arg, unsigned int *address)
+{
+  if (!symfile_objfile)
+    {
+      *address = -1;
+      return;
+    }
+
+  if (!strstr (arg, ":"))
+    {
+      struct minimal_symbol *minsym =
+	lookup_minimal_symbol (arg, NULL, symfile_objfile);
+
+      if (!minsym)
+	{
+	  *address = 0;
+	  return;
+	}
+
+      *address = SYMBOL_VALUE_ADDRESS (minsym);
+    }
+  else
+    {
+      struct symtabs_and_lines sals;
+      struct symtab_and_line sal;
+      unsigned int start_pc, end_pc;
+      int i;
+
+      init_sal (&sal);		/* initialize to zeroes */
+
+      sals = decode_line_spec_1 ((char *) arg, 0);
+      dont_repeat ();
+
+      if (find_line_pc_range
+	  (sals.sals[0], (CORE_ADDR *) & start_pc, (CORE_ADDR *) & end_pc))
+	*address = start_pc;
+      else
+	*address = 0;
+
+      xfree (sals.sals);
+    }
+
+  return;
+}
+
+static void
+shtdi_symbfromaddr (unsigned int address, char **symbol, char **filename,
+		    int *linenum)
+{
+  struct minimal_symbol *msymbol;
+  struct symtab_and_line sal;
+
+  *symbol = NULL;
+  *filename = NULL;
+  *linenum = -1;
+
+  if (!symfile_objfile)
+    return;
+
+  if ((msymbol = lookup_minimal_symbol_by_pc (address)))
+    {
+      *symbol = xstrdup (SYMBOL_PRINT_NAME (msymbol));
+      sal = find_pc_line (address, 0);
+      if (sal.symtab)
+	{
+	  *filename = xstrdup (sal.symtab->filename);
+	  *linenum = sal.line;
+	}
+      return;
+    }
+  return;
+}
