+static void
+shtdi_fetch_register (int regno)
+{
+  if (!shdbg_RtosSetGeneralThreadId (PIDGET (inferior_ptid)))
+    {
+      /* Not really an error if thread does not exist */
+    }
+  if (regno == -1)
+    {
+      unsigned char *regData = alloca (deprecated_register_bytes ());
+      if (!shdbg_ReadReg (-1, regData))
+	error (_("Unable to read target registers"));
+      for (regno = 0; regno < NUM_REGS; regno++, regData += MAX_REGISTER_SIZE)
+	regcache_raw_supply (current_regcache, regno, regData);
+    }
+  else
+    {
+      unsigned char *regData = alloca (MAX_REGISTER_SIZE);
+      if (!shdbg_ReadReg (regno, regData))
+	error (_("Unable to read target registers"));
+      regcache_raw_supply (current_regcache, regno, regData);
+    }
+}
+
+static void
+shtdi_store_register (int regno)
+{
+  if (!shdbg_RtosSetGeneralThreadId (PIDGET (inferior_ptid)))
+    {
+      /* Not really an error if thread does not exist */
+    }
+  if (regno == -1)
+    {
+      unsigned char *regData = alloca (deprecated_register_bytes ());
+      unsigned char *rp = regData;
+      for (regno = 0; regno < NUM_REGS; regno++, rp += MAX_REGISTER_SIZE)
+	deprecated_read_register_gen (regno, rp);
+      if (!shdbg_WriteReg (-1, regData))
+	error (_("Unable to write target registers"));
+    }
+  else
+    {
+      unsigned char *regData = alloca (MAX_REGISTER_SIZE);
+      deprecated_read_register_gen (regno, regData);
+      if (!shdbg_WriteReg (regno, regData))
+	error (_("Unable to write target registers"));
+    }
+  /* Cope with read only registers */
+  shtdi_fetch_register (regno);
+}
